<h2 class="signup-title">JokQuot Sign in</h2>

<div class="signup-form">
  <%= form_for :session, url: sessions_path do |f| %>

  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Sign In", class: "submit-user-button" %>
  </p>

  <% end %>
</div>
