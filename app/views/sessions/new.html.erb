<h2 class="signup-title">JokQuot Sign in</h2>

<%= form_for :session, url: sessions_path, class: "signup-form" do |f| %>

<p>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
</p>

<p>
  <%= f.label :password %><br>
  <%= f.password_field :password %>
</p>

<p>
  <%= f.submit "Sign In" %>
</p>

<% end %>
